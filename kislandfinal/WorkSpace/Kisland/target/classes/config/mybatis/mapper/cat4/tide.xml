<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="tide">
	<select id="selectTide" resultType="cat4.vo.TideVO"
		parameterType="int">
		select * from tide where port_no = #{port_no}
	</select>
</mapper>